    <script src="../../javascript/plugins/chart-area-demo.js"></script>
    <script src="../../javascript/plugins/chart-pie-demo.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/simple-datatables@latest" crossorigin="anonymous"></script>
