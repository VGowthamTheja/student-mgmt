<div class="col-lg-10">
  <h1>Blogs</h1>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Title</th>
        <th>Content</th>
        <th>Owner</th>
      </tr>
    </thead>
    <tbody>
      <% @blogs.each do |blog| %>
        <tr>
          <td><%= blog.title %></td>
          <td><%= blog.content %></td>
          <td> <%= blog.blog_owner||"N/A" %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
